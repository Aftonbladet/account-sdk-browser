{
  "name": "@schibsted/account-sdk-browser",
  "version": "3.1.0",
  "description": "Schibsted account SDK for browsers",
  "main": "index.js",
  "scripts": {
    "build": "./build.sh",
    "clean": "rimraf .cache coverage dist docs",
    "docs": "rimraf docs && jsdoc -c ./utils/jsdoc.js --verbose",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest",
    "precover": "npm run lint",
    "cover": "jest --coverage",
    "postcover": "codecov"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fetch-jsonp": "^1.1.3",
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.5",
    "core-js": "^3.6.4",
    "docdash": "git+https://github.com/torarvid/docdash.git#v0.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jsdoc": "^3.6.3",
    "node-fetch": "^2.6.0",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "whatwg-url": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/schibsted/account-sdk-browser.git"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "last 10 chrome major versions",
              "last 10 firefox major versions",
              "last 10 opera major versions",
              "last 2 safari major versions",
              "last 2 ios major versions",
              "last 2 ie major versions",
              "last 5 edge major versions"
            ]
          },
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": "3"
        }
      ]
    ]
  },
  "typings": "index.d.ts"
}
